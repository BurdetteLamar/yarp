Assignment:
  child_nodes:
    - node target
    - token operator
    - node value
  location: target->value
Binary:
  child_nodes:
    - node left
    - token operator
    - node right
  location: left->right
FloatLiteral:
  child_nodes:
    - token value
  location: value
Identifier:
  child_nodes:
    - token value
  location: value
IntegerLiteral:
  child_nodes:
    - token value
  location: value
OperatorAssignment:
  child_nodes:
    - node target
    - token operator
    - node value
  location: target->value
Program:
  child_nodes:
    - node statements
  location: statements
Statements:
  child_nodes:
    - node[] body
  location: body
VariableReference:
  child_nodes:
    - node value
  location: value
